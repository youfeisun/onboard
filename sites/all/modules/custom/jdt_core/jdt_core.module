/**
 * Implements hook_help().
 *
 * Displays help and module information.
 */
function jdt_core_help($path, $arg) {
  $output = "";
  switch ($path) {
    case "admin/help#jdt_core":
      $output = '<p>' . t("This is a test.") . '</p>';

      break;
  }

  return $output;
}
